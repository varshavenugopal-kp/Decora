             {{#if editCategoryData}} 
                <form method="post" action="/admin/category-edit/{{editCategoryData._id}}" style="display: flex;">
                
        <input class="form-control form-control-lg" type="text" name="category" id="category" style="width: 80%;" value="{{editCategoryData}}" placeholder="Enter Category">
         <button type="submit" class="btn amado-btn ">Update</button>
        </form>
        {{else}}
        <form method="post" action="/admin/category-add" style="display: flex;">
        <input class="form-control form-control-lg" type="text" name="category" id="category" style="width: 80%;" placeholder="Enter Category">
         <button type="submit" class="btn amado-btn ">Add</button>
        </form>
        <div>
            <p class="error text-danger">{{err}}</p>
        </div>

        {{/if}}

